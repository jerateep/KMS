<!DOCTYPE html>
<!--
This is a starter template page. Use this page to start your new project from
scratch. This page gets rid of all links and provides the needed markup only.
-->
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Key management System</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <link href="~/bower-package/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
    <!-- Font Awesome -->
    <link href="~/bower-package/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
    <!-- Ionicons -->
    <link href="~/bower-package/Ionicons/css/ionicons.min.css" rel="stylesheet" />
    <!-- Theme style -->
    <link href="~/bower-package/admin-lte/dist/css/AdminLTE.min.css" rel="stylesheet" />
    <link href="~/bower-package/admin-lte/dist/css/skins/skin-blue.min.css" rel="stylesheet" />
    <!-- Google Font -->
    <link href="https://fonts.googleapis.com/css?family=Athiti&display=swap" rel="stylesheet">
    <!--<link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">-->
    <!--DataPicker-->
    <!-- For Foundation -->
    <link href="~/bower-package/bootstrap-datepicker/css/datepicker-foundation.min.css" rel="stylesheet" />

    <!--Datatables-->
    <link href="~/bower-package/datatables.net/css/jquery.dataTables.min.css" rel="stylesheet" />
    <link href="~/bower-package/datatables.net/css/fixedHeader.dataTables.min.css" rel="stylesheet" />
    @*sweet alert*@

    <style>
        page[size="A4"] {
            font-family: 'Athiti', sans-serif;
            font-size: 12px;
            background: white;
/*            width: 27cm;*/
/*            height: 29.7cm;*/
            display: block;
            margin: 0 auto;
            margin-bottom: 0.5cm;
            /*box-shadow: 0 0 0.5cm rgba(0,0,0,0.5);*/
        }
        div.dataTables_wrapper {
            /*            width: 25cm;*/
            margin: 0 auto;
        }
        table > thead > tr > th {
            text-align: center;
/*            color: white;*/
            background-color: #1789bb;
        }
        table > tbody > tr > td {
            text-align: center;
        }
        .required-field:focus {
            border-color: red;
        }
        .cp-spinner{width:48px;height:48px;display:inline-block;box-sizing:border-box;position:relative}
        .cp-meter{border-radius:50%;border-top:solid 6px #86d160;border-right:solid 6px #86d160;border-bottom:solid 6px #86d160;border-left:solid 6px #86d160;width:48px;height:48px;display:inline-block;box-sizing:border-box}.cp-meter:before{border-radius:3px;content:" ";width:6px;height:12px;display:inline-block;box-sizing:border-box;background-color:#86d160;position:absolute;top:5px;left:16px;transform-origin:center bottom;animation:cp-meter-animate-before 1s linear infinite}@@keyframes cp-meter-animate-before{0%{transform:rotate(-45deg)}100%{transform:rotate(315deg)}}
        
        .center {
          text-align: center;
        }
        .bd-example-modal-lg .modal-dialog{
            display: table;
            position: relative;
            margin: 0 auto;
            top: calc(50% - 24px);
          }
        .bd-example-modal-lg .modal-dialog .modal-content{
            background-color: transparent;
            border: none;
          }

    </style>
</head>
<!--
BODY TAG OPTIONS:
=================
Apply one or more of the following classes to get the
desired effect
|---------------------------------------------------------|
| SKINS         | skin-blue                               |
|               | skin-black                              |
|               | skin-purple                             |
|               | skin-yellow                             |
|               | skin-red                                |
|               | skin-green                              |
|---------------------------------------------------------|
|LAYOUT OPTIONS | fixed                                   |
|               | layout-boxed                            |
|               | layout-top-nav                          |
|               | sidebar-collapse                        |
|               | sidebar-mini                            |
|---------------------------------------------------------|
-->
<body class="hold-transition skin-blue sidebar-collapse sidebar-mini">
    <div class="wrapper">
        @(await Component.InvokeAsync("TopMenu"))
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Main content -->
            <section class="content container-fluid">
                <page size="A4">
                    <div class="box box-default"></div>
                    <div class="box-body">
                        @RenderBody()
                    </div>
                </page>
            </section>
            <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->
        <!-- Main Footer -->
        <!--<footer class="main-footer">
            <strong>Copyright &copy; 2020 <a href="#">Demo Company</a>.</strong> All rights reserved
        </footer>-->
        <!-- Control Sidebar -->
        <aside class="control-sidebar control-sidebar-dark">
            <!-- Create the tabs -->
            <ul class="nav nav-tabs nav-justified control-sidebar-tabs">
                <li class="active"><a href="#control-sidebar-home-tab" data-toggle="tab"><i class="fa fa-home"></i></a></li>
                <li><a href="#control-sidebar-settings-tab" data-toggle="tab"><i class="fa fa-gears"></i></a></li>
            </ul>
        </aside>
        <div class="control-sidebar-bg"></div>
    </div>
    <!-- ./wrapper -->
    <!-- jQuery 3 -->
    <script src="~/bower-package/jquery/dist/jquery.min.js"></script>
    <script src="~/bower-package/jquery-ui/jquery-ui.min.js"></script>
    <!-- Bootstrap 3.3.7 -->
    <script src="~/bower-package/bootstrap/dist/js/bootstrap.min.js"></script>
    <!-- AdminLTE App -->
    <script src="~/bower-package/admin-lte/dist/js/adminlte.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/angular.js/1.3.4-build.3588/angular.js"></script>
    <script src="~/bower-package/bootstrap-datepicker/js/datepicker-full.min.js"></script>
    <script src="~/bower-package/bootstrap-datepicker/dist/js/bootstrap-datepicker.min.js"></script>
    <script src="~/bower-package/bootstrap-datepicker/js/locales/en-AU.js"></script>
    <!--Datatables-->
    <script src="~/bower-package/datatables.net/js/jquery.dataTables.min.js"></script>
    <script src="~/bower-package/datatables.net/js/dataTables.fixedHeader.min.js"></script>
    @*sweet alert*@
    @*sweet alert*@
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@10"></script>

    @RenderSection("Scripts", required: false)
</body>

</html>